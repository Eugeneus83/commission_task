{
    "name": "eugene/commission-task",
    "description": "Recruitment task",
    "keywords": [],
    "type": "project",
    "autoload": {
        "psr-4": {
            "Eugene\\CommissionTask\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Eugene\\CommissionTask\\Tests\\": "tests"
        }
    },
    "require": {
        "php": ">=8.0",
        "guzzlehttp/guzzle": "^7.8"
    },
    "config": {
        "sort-packages": true,
        "bin-dir": "bin"
    },
    "scripts": {
        "phpunit": "phpunit",
        "fix-cs": "php-cs-fixer fix",
        "test-cs": "php-cs-fixer fix --dry-run -v",
        "test": ["@phpunit", "@test-cs"]
    },
    "require-dev": {
        "phpunit/phpunit": "^11.0"
    }
}
